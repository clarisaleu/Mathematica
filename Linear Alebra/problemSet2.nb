 RowBox[{
  RowBox[{"(*", 
   RowBox[{"Set", "-", "up"}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"imageS", "=", 
    RowBox[{"RowReduce", "[", "S", "]"}]}], ";"}]}]], "Input",
