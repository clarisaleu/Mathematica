Cell[BoxData[{
 RowBox[{
  RowBox[{"constants", ":=", 
   RowBox[{"{", 
    RowBox[{
     SubscriptBox["c", "1"], ",", 
     SubscriptBox["c", "2"], ",", 
     SubscriptBox["c", "3"]}], "}"}]}], 
  ";"}], "\[IndentingNewLine]", "\"\<Matrix form of equation: W<c>=V\>\"", "\
\[IndentingNewLine]", 
 RowBox[{"StringForm", "[", 
  RowBox[{"\"\<````=``\>\"", ",", 
   RowBox[{"MatrixForm", "[", "W", "]"}], ",", 
