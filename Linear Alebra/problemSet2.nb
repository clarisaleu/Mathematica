Cell[BoxData[{
 RowBox[{"\"\<S: \>\"", " ", 
  RowBox[{"MatrixForm", "[", "S", "]"}]}], "\[IndentingNewLine]", 
 RowBox[{"\"\<Row Reduce S: \>\"", " ", 
  RowBox[{
  "MatrixForm", "[", "imageS", 
   "]"}]}], "\[IndentingNewLine]", "\"\<Defining the pivots of each column, \
the column space of \
\!\(\*StyleBox[\"S\",FontWeight->\"Bold\"]\)\!\(\*StyleBox[\" \",FontWeight->\
\"Bold\"]\)will be in \!\(\*FormBox[StyleBox[SuperscriptBox[\"R\", \
\"3\"],FontWeight->\"Bold\"],TraditionalForm]\) & is defined as:\>\"", "\
\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"calcS", "=", 
   RowBox[{"Transpose", "[", "S", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"StringForm", "[", 
  RowBox[{"\"\<col(S) = span{`` , `` ,  ``}\>\"", ",", 
   RowBox[{"MatrixForm", "[", 
    RowBox[{"calcS", "[", 
     RowBox[{"[", "1", "]"}], "]"}], "]"}], ",", 
   RowBox[{"MatrixForm", "[", 
    RowBox[{"calcS", "[", 
     RowBox[{"[", "2", "]"}], "]"}], "]"}], ",", 
   RowBox[{"MatrixForm", "[", 
    RowBox[{"calcS", "[", 
     RowBox[{"[", "3", "]"}], "]"}], "]"}]}], 
  "]"}], "\[IndentingNewLine]", "\"\<The basis for S can be expressed as the \
following equation, where a, b, & c are constants:\>\"", \
"\[IndentingNewLine]", 
 RowBox[{"StringForm", "[", 
  RowBox[{"\"\<a`` + b`` + c``\>\"", ",", 
   RowBox[{"MatrixForm", "[", 
    RowBox[{"calcS", "[", 
     RowBox[{"[", "1", "]"}], "]"}], "]"}], ",", 
   RowBox[{"MatrixForm", "[", 
    RowBox[{"calcS", "[", 
     RowBox[{"[", "2", "]"}], "]"}], "]"}], ",", 
   RowBox[{"MatrixForm", "[", 
    RowBox[{"calcS", "[", 
     RowBox[{"[", "3", "]"}], "]"}], "]"}]}], 
  "]"}], "\[IndentingNewLine]", "\"\<Finding the span(Where a, b, & c are \
constants)\nPutting S in the correct form: \>\"", "\[IndentingNewLine]", 
 RowBox[{"StringForm", "[", 
  RowBox[{"\"\<```` = ``\>\"", ",", 
   RowBox[{"MatrixForm", "[", 
    RowBox[{"Transpose", "[", "S", "]"}], "]"}], ",", 
   RowBox[{"MatrixForm", "[", "constants", "]"}], ",", 
   RowBox[{"MatrixForm", "[", "f", "]"}]}], "]"}], "\[IndentingNewLine]", 
 RowBox[{"StringForm", "[", 
  RowBox[{
  "\"\<The determinant of the left-hand side matrix is ``, which is not zero, \
so it is invertible and there is a solution for <c>.\>\"", ",", 
   RowBox[{"Det", "[", 
    RowBox[{"Transpose", "[", "S", "]"}], "]"}]}], 
  "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"sav", "=", 
   RowBox[{"Inverse", "[", 
    RowBox[{"Transpose", "[", "S", "]"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", "\"\<Solving directly:\>\"", "\
\[IndentingNewLine]", 
 RowBox[{"StringForm", "[", 
  RowBox[{
  "\"\<``=``; where a,b, & c are constants and can be all real numbers\>\"", 
   ",", 
   RowBox[{"MatrixForm", "[", "constants", "]"}], ",", 
   RowBox[{"MatrixForm", "[", 
    RowBox[{"sav", ".", "f"}], "]"}]}], "]"}]}], "Input",
 CellOpen->False,
