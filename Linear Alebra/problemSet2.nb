Cell[BoxData[
 RowBox[{
  RowBox[{"(*", "Given", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     SubscriptBox["p", "1"], "=", 
     RowBox[{"1", "-", "x", "+", 
      RowBox[{"2", 
       SuperscriptBox["x", "2"]}]}]}], ";", " ", 
    RowBox[{
     SubscriptBox["p", "2"], "=", 
     RowBox[{"3", "+", "x"}]}], ";", 
    RowBox[{
     SubscriptBox["p", "3"], "=", 
     RowBox[{"5", "-", "x", "+", 
      RowBox[{"4", 
       SuperscriptBox["x", "2"]}]}]}], ";", " ", 
    RowBox[{
     SubscriptBox["p", "4"], "=", 
     RowBox[{
      RowBox[{"-", "2"}], "-", 
      RowBox[{"2", "x"}], "+", 
      RowBox[{"2", 
       SuperscriptBox["x", "2"]}]}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"coeff", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
