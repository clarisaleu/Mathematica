Cell[TextData[Cell[BoxData[
 RowBox[{"L", "=", 
  RowBox[{"(", GridBox[{
     {
      SubscriptBox["a", "1"], 
      SubscriptBox["a", "2"], 
      SubscriptBox["a", "3"], "...", 
      SubscriptBox["a", 
       RowBox[{"n", "-", "1"}]], 
      SubscriptBox["a", "n"]},
     {
      SubscriptBox["b", "1"], "0", "0", "...", "0", "0"},
     {"0", 
      SubscriptBox["b", "2"], "0", "...", "0", "0"},
     {".", ".", ".", " ", ".", "."},
     {".", ".", ".", " ", ".", "."},
     {".", ".", ".", " ", ".", "."},
     {"0", "0", "0", "...", 
      SubscriptBox["b", 
       RowBox[{"n", "-", "1"}]], "0"}
    }], ")"}]}]],
 CellChangeTimes->{{3.7304899756077194`*^9, 3.730490096235304*^9}, {
