 RowBox[{"Style", "[", 
  RowBox[{
   RowBox[{"Framed", "[", 
    RowBox[{"Framed", "[", 
     RowBox[{"Style", "[", 
      RowBox[{
       RowBox[{
       "StringForm", "[", "\"\<Leslie Matrix for Given Population Data\>\"", 
        "]"}], ",", 
       RowBox[{"FontFamily", "\[Rule]", "\"\<Tahoma\>\""}], ",", 
       RowBox[{"FontSize", "\[Rule]", "18"}], ",", 
       RowBox[{"FontWeight", "\[Rule]", "Bold"}]}], "]"}], "]"}], "]"}], ",", 
   RowBox[{"TextAlignment", "\[Rule]", "Center"}]}], 
  "]"}], "\[IndentingNewLine]", 
 RowBox[{"Style", "[", 
  RowBox[{
   RowBox[{"Framed", "[", 
    RowBox[{"StringForm", "[", 
     RowBox[{"\"\<L = ``\>\"", ",", 
      RowBox[{"MatrixForm", "[", 
       RowBox[{"L", "=", 
        RowBox[{"Prepend", "[", 
         RowBox[{
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{"Insert", "[", 
             RowBox[{
