Cell[TextData[{
 StyleBox["\tThis system is a first order linear differential equation of the \
form, ",
  FontFamily->"Tahoma",
  FontWeight->"Plain"],
 StyleBox["<x\[CloseCurlyQuote]> = R<x>",
  FontFamily->"Tahoma",
  FontWeight->"Bold"],
 StyleBox["; Where ",
  FontFamily->"Tahoma",
  FontWeight->"Plain"],
 StyleBox["R",
  FontFamily->"Tahoma",
  FontWeight->"Bold"],
 StyleBox[" is a matrix with constant coefficients. As ",
  FontFamily->"Tahoma",
  FontWeight->"Plain"],
 StyleBox["R",
  FontFamily->"Tahoma",
  FontWeight->"Bold"],
 StyleBox[" is not a diagonal matrix, we can solve the system using \
Diagonalization. We will use the same process as described in Problem 1.",
  FontFamily->"Tahoma",
  FontWeight->"Plain"]
}], "Text",
 CellChangeTimes->{{3.730476252091896*^9, 3.7304762622983704`*^9}},
 FontSize->16,ExpressionUUID->"9c0d89fd-4bac-4240-8a4a-cdd04e9af11f"],

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{"Seting", " ", "up", " ", "problem"}], "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   StyleBox[
    RowBox[{
     RowBox[{"R", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"-", "2"}], ",", 
          RowBox[{
           RowBox[{"-", "5"}], "/", "2"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"10", ",", 
          RowBox[{"-", "2"}]}], "}"}]}], "}"}]}], ";"}],
    FontWeight->"Plain"], 
   StyleBox["\[IndentingNewLine]",
    FontWeight->"Plain"], 
   StyleBox[
    RowBox[{
     RowBox[{"eigenValuesR", " ", "=", " ", 
      RowBox[{"Eigenvalues", "[", "R", "]"}]}], ";"}],
    FontWeight->"Plain"], 
   StyleBox["\[IndentingNewLine]",
    FontWeight->"Plain"], 
   StyleBox[
    RowBox[{
     RowBox[{"eigenVectorsR", " ", "=", " ", 
      RowBox[{"Eigenvectors", "[", "R", "]"}]}], ";"}],
    FontWeight->"Plain"], 
   StyleBox["\[IndentingNewLine]",
    FontWeight->"Plain"], 
   StyleBox[
    RowBox[{
     RowBox[{"PR", " ", "=", " ", 
      RowBox[{"Transpose", "[", "eigenVectorsR", "]"}]}], ";"}],
    FontWeight->"Plain"], 
   StyleBox["\[IndentingNewLine]",
    FontWeight->"Plain"], 
   StyleBox[
    RowBox[{"Style", "[", 
     RowBox[{
      RowBox[{"Framed", "[", 
       RowBox[{"Style", "[", 
        RowBox[{
         RowBox[{"StringForm", "[", 
          RowBox[{"\"\<R= `` \\n P =``\>\"", ",", 
           RowBox[{"MatrixForm", "[", "R", "]"}], ",", 
           RowBox[{"MatrixForm", "[", "PR", "]"}]}], "]"}], ",", 
         RowBox[{"TextAlignment", "\[Rule]", "Center"}], ",", 
         RowBox[{"FontFamily", "\[Rule]", "\"\<Tahoma\>\""}], ",", 
         RowBox[{"FontSize", "\[Rule]", "18"}]}], "]"}], "]"}], ",", 
      RowBox[{"TextAlignment", "\[Rule]", "Center"}], ",", 
      RowBox[{"FontWeight", "\[Rule]", "Bold"}]}], "]"}],
    FontWeight->"Plain"], "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{
      RowBox[{"Solving", " ", "for"}], " ", "<", "u", ">"}], ",", " ", 
     RowBox[{
     "essentially", " ", "the", " ", "eigenvalues", " ", "of", " ", "R"}]}], 
    "*)"}], "\[IndentingNewLine]", 
   StyleBox[
    RowBox[{
     RowBox[{"DMatrixR", " ", "=", "  ", 
      RowBox[{
       RowBox[{"Inverse", "[", "PR", "]"}], ".", "R", ".", "PR"}]}], ";"}],
    FontWeight->"Plain"], 
   StyleBox["\[IndentingNewLine]",
    FontWeight->"Plain"], 
   StyleBox[
    RowBox[{
     RowBox[{"uSolutionsR", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         SubscriptBox["c", "1"], "*", 
         RowBox[{"Exp", "[", 
          RowBox[{
           RowBox[{"eigenValuesR", "[", 
            RowBox[{"[", "1", "]"}], "]"}], "*", "x"}], "]"}]}], ",", 
        RowBox[{
         SubscriptBox["c", "2"], "*", 
         RowBox[{"Exp", "[", 
          RowBox[{
           RowBox[{"eigenValuesR", "[", 
            RowBox[{"[", "2", "]"}], "]"}], "*", "x"}], "]"}]}]}], "}"}]}], 
     ";"}],
    FontWeight->"Plain"], 
   StyleBox["\[IndentingNewLine]",
    FontWeight->"Plain"], 
   StyleBox[
    RowBox[{"Style", "[", 
     RowBox[{
      RowBox[{"Framed", "[", 
       RowBox[{"Style", "[", 
        RowBox[{
         RowBox[{"StringForm", "[", 
          RowBox[{"\"\<Of the form: <u'>=D<u>\\n `` = ````\>\"", ",", 
           RowBox[{"MatrixForm", "[", "uPrimeDisplay2", "]"}], ",", 
           RowBox[{"MatrixForm", "[", "DMatrixR", "]"}], ",", 
           RowBox[{"MatrixForm", "[", "uDisplay2", "]"}]}], "]"}], ",", 
         RowBox[{"FontSize", "\[Rule]", "18"}], ",", 
         RowBox[{"FontFamily", "\[Rule]", "\"\<Tahoma\>\""}]}], "]"}], "]"}], 
      ",", 
      RowBox[{"TextAlignment", "\[Rule]", "Center"}], ",", 
      RowBox[{"FontWeight", "\[Rule]", "Bold"}]}], "]"}],
    FontWeight->"Plain"], 
   StyleBox["\[IndentingNewLine]",
    FontWeight->"Plain"], 
   StyleBox[
    RowBox[{"Style", "[", 
     RowBox[{
      RowBox[{"Framed", "[", 
       RowBox[{"Style", "[", 
        RowBox[{
         RowBox[{
         "StringForm", "[", 
          "\"\<Reading off solutions, given the general solution of \
<y'>=A<y>; where A is a Diagonal constant coefficient matrix: \\n \
\!\(\*SubscriptBox[\(u\), \(1\)]\)=\!\(\*SubscriptBox[\(c\), \
\(1\)]\)\!\(\*SuperscriptBox[\(e\), \(\((\(-2\) + 5  i)\) x\)]\) \\n \
\!\(\*SubscriptBox[\(u\), \(2\)]\)=\!\(\*SubscriptBox[\(c\), \
\(2\)]\)\!\(\*SuperscriptBox[\(e\), \(\((\(-2\) - 5  i)\) x\)]\) \\n *These \
are also just the eigenvalues of matrix R.*\>\"", "]"}], ",", 
         RowBox[{"FontFamily", "\[Rule]", "\"\<Tahoma\>\""}], ",", 
         RowBox[{"FontSize", "\[Rule]", "18"}]}], "]"}], "]"}], ",", 
      RowBox[{"TextAlignment", "\[Rule]", "Center"}], ",", 
      RowBox[{"FontWeight", "\[Rule]", "Bold"}]}], "]"}],
    FontWeight->"Plain"], "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"Relating", " ", "<", "u", ">", " ", 
     RowBox[{"back", " ", "to"}], " ", "<", "y", ">"}], "*)"}], 
   "\[IndentingNewLine]", 
   StyleBox[
    RowBox[{
     RowBox[{"ySolutionsR", "=", 
      RowBox[{"PR", ".", "uSolutionsR"}]}], ";"}],
    FontWeight->"Plain"], 
   StyleBox["\[IndentingNewLine]",
    FontWeight->"Plain"], 
   StyleBox[
    RowBox[{"Style", "[", 
     RowBox[{
      RowBox[{"Framed", "[", 
       RowBox[{"StringForm", "[", 
        RowBox[{"\"\<Of the form: <y>=P<u> \\n`` = ````\>\"", ",", 
         RowBox[{"MatrixForm", "[", "xDisplay", "]"}], ",", 
         RowBox[{"MatrixForm", "[", "PR", "]"}], ",", 
         RowBox[{"MatrixForm", "[", "uSolutionsR", "]"}]}], "]"}], "]"}], ",", 
      RowBox[{"TextAlignment", "\[Rule]", "Center"}], ",", 
      RowBox[{"FontFamily", "\[Rule]", "\"\<Tahoma\>\""}], ",", 
      RowBox[{"FontWeight", "\[Rule]", "Bold"}], ",", 
      RowBox[{"FontSize", "\[Rule]", "18"}]}], "]"}],
    FontWeight->"Plain"], 
   StyleBox["\[IndentingNewLine]",
    FontWeight->"Plain"], 
   StyleBox["\[IndentingNewLine]",
    FontWeight->"Plain"], 
   StyleBox[
    RowBox[{"Style", "[", 
     RowBox[{
      RowBox[{"Framed", "[", 
       RowBox[{"StringForm", "[", 
        RowBox[{
        "\"\<Therefore: \\n \!\(\*SubscriptBox[\(x\), \(1\)]\)= `` \\n \
\!\(\*SubscriptBox[\(x\), \(2\)]\)= `` \\n \>\"", ",", 
         RowBox[{"ySolutionsR", "[", 
          RowBox[{"[", "1", "]"}], "]"}], ",", 
         RowBox[{"ySolutionsR", "[", 
          RowBox[{"[", "2", "]"}], "]"}]}], "]"}], "]"}], ",", 
      RowBox[{"TextAlignment", "\[Rule]", "Center"}], ",", 
      RowBox[{"FontWeight", "\[Rule]", "Bold"}], ",", 
      RowBox[{"FontFamily", "\[Rule]", "\"\<Tahoma\>\""}], ",", 
      RowBox[{"FontSize", "\[Rule]", "18"}]}], "]"}],
    FontWeight->"Plain"], "\[IndentingNewLine]", 
   StyleBox["\[IndentingNewLine]",
    FontWeight->"Plain"]}]}]], "Input",
